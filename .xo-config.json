{
	"envs": [
		"node",
		"es2022",
		"browser"
	],
	"ignores": [
		"**/*deprecated*",
		"**/*cred*",
		"**/*.js",
		"**/*.cjs",
		"**/*.d.ts",
		"**/*.json",
		"**/*.ignore*",
		"node_modules",
		"serverless_sdk",
		".history",
		"lib",
		"dist",
		"public",
		"coverage",
		".serverless"
	],
	"space": true,
	"trailingComma": false,
	"bracketSpacing": true,
	"singleQuote": false,
	"rules": {
		"curly": [
			2,
			"multi-or-nest",
			"consistent"
		],
		"complexity": [
			2,
			25
		],
		"unicorn/no-new-array": 0,
		"unicorn/prefer-module": 0,
		"unicorn/prefer-node-protocol": 0,
		"unicorn/no-array-reduce": 0,
		"unicorn/no-array-callback-reference": 0,
		"unicorn/prefer-object-from-entries": 1,
		"unicorn/no-array-for-each": 1,
		"unicorn/filename-case": [
			2,
			{
				"cases": {
					"kebabCase": true,
					"snakeCase": true
				}
			}
		],
		"unicorn/no-object-as-default-parameter": 1,
		"unicorn/no-process-exit": 1,
		"unicorn/no-thenable": 2,
		"unicorn/prefer-native-coercion-functions": [
			2
		],
		"unicorn/prevent-abbreviations": [
			2,
			{
				"allowList": {
					"env": true,
					"master_password": true
				}
			}
		],
		"import/no-anonymous-default-export": 0,
		"import/no-unassigned-import": 0,
		"import/extensions": 0,
		"import/order": 1,
		"import/first": 0,
		"node/prefer-global/process": 0,
		"object-curly-spacing": [
			2,
			"always"
		],
		"no-template-curly-in-string": 2,
		"object-curly-newline": 2,
		"template-curly-spacing": [
			2,
			"always"
		],
		"no-trailing-spaces": 0,
		"no-negated-condition": 0,
		"no-empty": 0,
		"no-return-assign": 1,
		"no-void": 0,
		"no-useless-catch": 2,
		"no-mixed-spaces-and-tabs": [
			2,
			"smart-tabs"
		],
		"no-implicit-coercion": [
			2,
			{
				"boolean": false
			}
		],
		"new-cap": 0,
		"max-params": [
			2,
			24
		],
		"array-callback-return": 0,
		"@typescript-eslint/quotes": [
			2,
			"backtick"
		],
		"@typescript-eslint/default-param-last": 1,
		"@typescript-eslint/prefer-reduce-type-parameter": 0,
		"@typescript-eslint/dot-notation": 0,
		"@typescript-eslint/no-base-to-string": 0,
		"@typescript-eslint/no-unused-vars": 1,
		"@typescript-eslint/no-floating-promises": 0,
		"@typescript-eslint/consistent-type-assertions": [
			1,
			{
				"assertionStyle": "as",
				"objectLiteralTypeAssertions": "allow-as-parameter"
			}
		],
		"@typescript-eslint/no-dynamic-delete": 0,
		"@typescript-eslint/object-curly-spacing": [
			2,
			"always"
		],
		"@typescript-eslint/no-implicit-any-catch": 0,
		"@typescript-eslint/no-explicit-any": 1,
		"@typescript-eslint/no-unsafe-return": 0,
		"@typescript-eslint/no-unsafe-call": 0,
		"@typescript-eslint/no-unsafe-assignment": 0,
		"@typescript-eslint/naming-convention": [
			1,
			{
				"selector": "default",
				"format": [
					"camelCase",
					"snake_case",
					"UPPER_CASE",
					"PascalCase"
				],
				"leadingUnderscore": "allowSingleOrDouble",
				"trailingUnderscore": "allowSingleOrDouble"
			},
			{
				"selector": [
					"class",
					"interface",
					"typeAlias",
					"typeParameter"
				],
				"format": [
					"camelCase",
					"PascalCase"
				],
				"leadingUnderscore": "allowSingleOrDouble",
				"trailingUnderscore": "allowSingleOrDouble"
			}
		],
		"@typescript-eslint/restrict-template-expressions": 0,
		"@typescript-eslint/no-throw-literal": 1,
		"@typescript-eslint/member-ordering": 0,
		"@typescript-eslint/unified-signatures": 1,
		"@typescript-eslint/no-redundant-type-constituents": 1,
		"@typescript-eslint/no-unused-expressions": [
			2,
			{
				"allowShortCircuit": true,
				"allowTernary": true
			}
		],
		"@typescript-eslint/ban-types": [
			2,
			{
				"types": {
					"null": false,
					"{}": false,
					"object": false
				},
				"extendDefaults": true
			}
		],
		"@typescript-eslint/prefer-nullish-coalescing": [
			2,
			{
				"ignoreTernaryTests": false,
				"ignoreConditionalTests": false,
				"ignoreMixedLogicalExpressions": false
			}
		],
		"@typescript-eslint/class-literal-property-style": 0,
		"n/prefer-global/process": 0
	},
	"semicolon": false
}